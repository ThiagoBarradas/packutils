<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Thiago Barradas</Authors>
    <Description>Many utilities like: enumeration, hash, json (extension newtonsoft), regex, signature, string, uri, dictionary, etc</Description>
    <PackageTags>enumeration, hash, json, newtonsoft, regex, signature, string, uri, util, utils, utility, utilities, crypt, dictionary, casting, cast</PackageTags>
    <RepositoryUrl>https://github.com/ThiagoBarradas/packutils</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/ThiagoBarradas/packutils/blob/master/LICENSE</PackageLicenseUrl>
    <Company></Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://i.imgur.com/bWz72sT.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/ThiagoBarradas/packutils</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
